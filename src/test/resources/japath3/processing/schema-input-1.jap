assert(
	every(*,
		&.match('a|a1|b')),
	a.type(String),
	a1.assert(
		every(*,
			&.match('a11|\Qa22"-ยง$%\E')),
		a11.type(Number),
		`a22"-ยง$%`.type(String)),
	b.every(*,
		or(
			assert(
				every(*,
					&.match('c|d')),
				c.type(Number),
				d.every(*,
					or(
						assert(
							every(*,
								&.match('Y|y')),
							Y.type(String),
							y.type(Number)),
						assert(
							every(*,
								&.match('Z|y')),
							Z.type(String),
							y.type(Number)),
						assert(
							every(*,
								&.match('end')),
							end.type(String))))),
			assert(
				every(*,
					&.match('c|c_|f')),
				c.type(Number),
				c_.type(Number),
				f.every(*,
					or(
						type(String),
						assert(
							every(*,
								&.match('u|v')),
							u.type(String),
							v.type(Number))))))))