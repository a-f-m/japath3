def(
	h0(),
	assert(
		every(*,
			&.match('a11|\Qa22"-ยง$%\E')),
		a11.type(Number),
		`a22"-ยง$%`.type(String)))
.def(
	h1(),
	assert(
		every(*,
			&.match('Y|y')),
		Y.type(String),
		y.type(Number)))
.def(
	h2(),
	assert(
		every(*,
			&.match('Z|y')),
		Z.type(String),
		y.type(Number)))
.def(
	h3(),
	assert(
		every(*,
			&.match('end')),
		end.type(String)))
.def(
	h4(),
	assert(
		every(*,
			&.match('c|d')),
		c.type(Number),
		d.every(*,
			or(
				h1(),
				h2(),
				h3()))))
.def(
	h5(),
	assert(
		every(*,
			&.match('u|v')),
		u.type(String),
		v.type(Number)))
.def(
	h6(),
	assert(
		every(*,
			&.match('u|v')),
		u.type(String),
		v.type(Number)))
.def(
	h7(),
	assert(
		every(*,
			&.match('c|c_|f')),
		c.type(Number),
		c_.type(Number),
		f.every(*,
			or(
				type(String),
				h5(),
				h6()))))
.def(
	h8(),
	assert(
		every(*,
			&.match('a|a1|b')),
		a.type(String),
		a1.h0(),
		b.every(*,
			or(
				h4(),
				h7())))).h8()